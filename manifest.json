{
  "manifest_version": 2,

  "name": "eBS Tool",
  "description": "eBS Tool To Help developers Save Links in Chrome Browser ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
	 "matches": ["http://*/*","https://*/*"]
  },
  "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/",
		"storage" 
    ],
	"icons": { "16": "icon.png",
           "48": "iconT.png",
          "128": "iconT.png" }
  
}
